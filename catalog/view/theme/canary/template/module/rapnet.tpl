            $get_cut_to = queryStr['cut'].split(',')[1];
            $url_cut = '&cut='+queryStr['cut'].split(',')[0]+','+queryStr['cut'].split(',')[1];
        } else {
            $get_cut_from = 0;
            $get_cut_to = 4;
