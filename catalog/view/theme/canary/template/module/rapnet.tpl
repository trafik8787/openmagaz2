        //get parse url
        function parseQueryString (strQuery) {
            var i,
                    tmp     = [],
                    tmp2    = [],
                    objRes   = {};
            if (strQuery != '') {
                tmp = (strQuery.substr(1)).split('&');
                if (tmp.length == 1) {
                    tmp = strQuery.substr(1).split('?');
                }
                for (i = 0; i < tmp.length; i += 1) {
                    tmp2 = tmp[i].split('=');
                    if (tmp2[0]) {
                        objRes[tmp2[0]] = tmp2[1];
                    }
                }
            }
            return objRes;
        }




